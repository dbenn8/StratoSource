    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>FundRequest_AccountNameIsNotChanged</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Account_master__c )</errorConditionFormula>
        <errorMessage>Account Name should not be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FundRequest_AmountIsPositive</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISBLANK(Estimated_Red_Hat_Funding_Requested__c), Estimated_Red_Hat_Funding_Requested__c &lt; 0)</errorConditionFormula>
        <errorDisplayField>Estimated_Red_Hat_Funding_Requested__c</errorDisplayField>
        <errorMessage>Please enter a positive number for Estimated Red Hat Funding Requested</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FundRequest_AmountIsWithinBudget</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISPICKVAL( Approval_Status__c , &apos;Draft&apos;), Estimated_Red_Hat_Funding_Requested__c &gt; Budget__r.Available_Budget__c )</errorConditionFormula>
        <errorMessage>Please ensure the Estimated Red Hat Funding Requested is less than the Available Budget for the Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FundRequest_AmountIsWithinExpenditure</fullName>
        <active>true</active>
        <errorConditionFormula>Estimated_Red_Hat_Funding_Requested__c &gt; Total_Expenditure_of_Activity__c</errorConditionFormula>
        <errorDisplayField>Estimated_Red_Hat_Funding_Requested__c</errorDisplayField>
        <errorMessage>Please ensure the Estimated Red Hat Funding Requested is less than the Total Expenditure of the Activity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FundRequest_BudgetIsNotChanged</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Budget__c )</errorConditionFormula>
        <errorMessage>Channel Budget name should not be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FundRequest_IsActivityDateWithinFQ</fullName>
        <active>true</active>
        <description>Ensures the Activity dates are withing the FQ of the associated Budget</description>
        <errorConditionFormula>OR
(
 Activity_Start_Date__c &lt; Budget__r.Start_Date__c ,
 Activity_Start_Date__c &gt; Budget__r.End_Date__c,
 Activity_End_Date__c &gt;  Budget__r.End_Date__c,
 Activity_End_Date__c &lt; Budget__r.Start_Date__c
)</errorConditionFormula>
        <errorMessage>The Activity Start and End dates cannot be outside of the Fiscal Quarter defined on the associated MDF Budget.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FundRequest_IsExpired</fullName>
        <active>true</active>
        <description>Prevents editing on an expired Fund Request</description>
        <errorConditionFormula>AND(NOT(ISCHANGED(Approval_Status__c)),ISPICKVAL( Approval_Status__c, &quot;Expired&quot;))</errorConditionFormula>
        <errorMessage>This Fund Request is expired and no longer available for editing.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FundRequest_IsLocked</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL($User.UserType, &apos;PowerPartner&apos;),  NOT(ISPICKVAL(Approval_Status__c,&apos;Draft&apos;)), NOT(ISPICKVAL(Approval_Status__c,&apos;Rejected&apos;)))</errorConditionFormula>
        <errorMessage>This fund request has entered the approval process and can no longer be edited.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FundRequest_TotalExpenditureIsPositive</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISBLANK(Total_Expenditure_of_Activity__c), Total_Expenditure_of_Activity__c &lt; 0)</errorConditionFormula>
        <errorDisplayField>Total_Expenditure_of_Activity__c</errorDisplayField>
        <errorMessage>Please enter a positive number for Total Expenditure of Activity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FundRequest_ValidateDates</fullName>
        <active>true</active>
        <description>Ensures the Activity start date is before or equal to the end date</description>
        <errorConditionFormula>Activity_End_Date__c &lt;  Activity_Start_Date__c</errorConditionFormula>
        <errorMessage>Please ensure the End Date occurs after the Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FundRequest_ValidateTCs</fullName>
        <active>false</active>
        <errorConditionFormula>I_Agree_to_the_Terms_and_Conditions__c ==false</errorConditionFormula>
        <errorDisplayField>I_Agree_to_the_Terms_and_Conditions__c</errorDisplayField>
        <errorMessage>Please check &quot;I Agree to the Terms and Conditions to continue&quot;.</errorMessage>
    </validationRules>
